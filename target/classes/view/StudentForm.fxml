<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="StudentForm" prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.gdse.controller.StudentFormController">
   <children>
      <Label layoutX="229.0" layoutY="27.0" prefHeight="30.0" prefWidth="141.0" text="Student Form">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtName" layoutX="209.0" layoutY="93.0" onAction="#txtNameOnAction" promptText="name" />
      <TextField fx:id="txtAge" layoutX="210.0" layoutY="149.0" onAction="#txtAgeOnAction" promptText="age" />
      <TextField fx:id="txtAdderss" layoutX="210.0" layoutY="200.0" onAction="#txtAddressOnAction" promptText="address" />
      <Label layoutX="86.0" layoutY="97.0" text="Name" />
      <Label layoutX="82.0" layoutY="153.0" text="Age" />
      <Label layoutX="86.0" layoutY="204.0" text="Address" />
      <Button fx:id="btnSave" layoutX="285.0" layoutY="330.0" mnemonicParsing="false" onAction="#btnSaveOnAction" text="Save" />
      <Button fx:id="btnDelete" layoutX="345.0" layoutY="330.0" mnemonicParsing="false" onAction="#btnDeleteOnAction" text="Delete" />
      <Button fx:id="btnUpdate" layoutX="423.0" layoutY="330.0" mnemonicParsing="false" onAction="#btnUpdateOnAction" text="Update" />
   </children>
</AnchorPane>
